CREATE TABLE CUSTOMER (
		CUSTOMERID INTEGER NOT NULL,
		CUSTOMERFIRSTNAME VARCHAR(255),
		CUSTOMERLASTNAME VARCHAR(255),
		DATEOFPURCHASE VARCHAR(255),
		CONTACTINFORMATION_CUSTOMERCONTACTID INTEGER,
		INDUSTRYID_INDUSTRYID INTEGER,
		PRINTER_ID_PRINTERID INTEGER,
		STAFFID_STAFFID INTEGER
	);
	
CREATE INDEX SQL201007022330650 ON CUSTOMER (INDUSTRYID_INDUSTRYID ASC);

CREATE INDEX SQL201007022330750 ON CUSTOMER (PRINTER_ID_PRINTERID ASC);

CREATE INDEX SQL201007022330860 ON CUSTOMER (STAFFID_STAFFID ASC);

CREATE UNIQUE INDEX SQL201007022329180 ON CUSTOMER (CUSTOMERID ASC);

CREATE INDEX SQL201007022330530 ON CUSTOMER (CONTACTINFORMATION_CUSTOMERCONTACTID ASC);

ALTER TABLE CUSTOMER ADD CONSTRAINT SQL201007022329180 PRIMARY KEY (CUSTOMERID);

