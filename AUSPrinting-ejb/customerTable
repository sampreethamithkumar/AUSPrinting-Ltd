	REFERENCES PRINTER (PRINTERID);

ALTER TABLE CUSTOMER ADD CONSTRAINT CSTMERSTFFDSTAFFID FOREIGN KEY (STAFFID_STAFFID)
	REFERENCES STAFF (STAFFID);

