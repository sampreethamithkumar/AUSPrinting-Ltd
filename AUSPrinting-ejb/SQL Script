/*Staff table creation*/
CREATE TABLE STAFF (
		STAFFID INTEGER NOT NULL,
		STAFFEMAIL VARCHAR(255),
		STAFFFNAME VARCHAR(255),
		STAFFLNAME VARCHAR(255),
		STAFFPHONENUMBER BIGINT,
		STAFFTFN VARCHAR(255),
		POSTCODE VARCHAR(255),
		STATE VARCHAR(255),
		STREETADDRESS VARCHAR(255),
		STREETNUMBER VARCHAR(255),
		SUBURB VARCHAR(255)
	);

CREATE UNIQUE INDEX SQL201007022329670 ON STAFF (STAFFID ASC);

ALTER TABLE STAFF ADD CONSTRAINT SQL201007022329670 PRIMARY KEY (STAFFID);

/*Customer table creation*/
CREATE TABLE CUSTOMER (
		CUSTOMERID INTEGER NOT NULL,
		CUSTOMERFIRSTNAME VARCHAR(255),
		CUSTOMERLASTNAME VARCHAR(255),
		DATEOFPURCHASE DATE,
		CONTACTINFORMATION_CUSTOMERCONTACTID INTEGER,
		INDUSTRYID_INDUSTRYID INTEGER,
		PRINTER_ID_PRINTERID INTEGER,
		STAFFID_STAFFID INTEGER
	);

CREATE INDEX SQL201007022330650 ON CUSTOMER (INDUSTRYID_INDUSTRYID ASC);

CREATE INDEX SQL201007022330750 ON CUSTOMER (PRINTER_ID_PRINTERID ASC);

CREATE INDEX SQL201007022330860 ON CUSTOMER (STAFFID_STAFFID ASC);

CREATE UNIQUE INDEX SQL201007022329180 ON CUSTOMER (CUSTOMERID ASC);

CREATE INDEX SQL201007022330530 ON CUSTOMER (CONTACTINFORMATION_CUSTOMERCONTACTID ASC);

ALTER TABLE CUSTOMER ADD CONSTRAINT SQL201007022329180 PRIMARY KEY (CUSTOMERID);

ALTER TABLE CUSTOMER ADD CONSTRAINT CSTMNDSTRYDNDSTRYD FOREIGN KEY (INDUSTRYID_INDUSTRYID)
	REFERENCES INDUSTRY (INDUSTRYID);

ALTER TABLE CUSTOMER ADD CONSTRAINT CSTMRPRNTRDPRNTRID FOREIGN KEY (PRINTER_ID_PRINTERID)
	REFERENCES PRINTER (PRINTERID);

ALTER TABLE CUSTOMER ADD CONSTRAINT CSTMERSTFFDSTAFFID FOREIGN KEY (STAFFID_STAFFID)
	REFERENCES STAFF (STAFFID);

ALTER TABLE CUSTOMER ADD CONSTRAINT CNTCTNFRMTNCSTMRCN FOREIGN KEY (CONTACTINFORMATION_CUSTOMERCONTACTID)
	REFERENCES CUSTOMERCONTACTINFORMATION (CUSTOMERCONTACTID);
	
/* Customer Contact Information table creation */
CREATE TABLE CUSTOMERCONTACTINFORMATION (
		CUSTOMERCONTACTID INTEGER NOT NULL,
		CUSTOMERDOB DATE,
		CUSTOMEREMAIL VARCHAR(255),
		CUSTOMERPHONENUMBER BIGINT,
		CUSTOMERTFN VARCHAR(255),
		POSTCODE VARCHAR(255),
		STATE VARCHAR(255),
		STREETADDRESS VARCHAR(255),
		STREETNUMBER VARCHAR(255),
		SUBURB VARCHAR(255)
	);

CREATE UNIQUE INDEX SQL201007022329460 ON CUSTOMERCONTACTINFORMATION (CUSTOMERCONTACTID ASC);

ALTER TABLE CUSTOMERCONTACTINFORMATION ADD CONSTRAINT SQL201007022329460 PRIMARY KEY (CUSTOMERCONTACTID);


/*Industry table creation */
CREATE TABLE INDUSTRY (
		INDUSTRYID INTEGER NOT NULL,
		INDUSTRYTYPE VARCHAR(255)
	);

CREATE UNIQUE INDEX SQL201007022329850 ON INDUSTRY (INDUSTRYID ASC);

ALTER TABLE INDUSTRY ADD CONSTRAINT SQL201007022329850 PRIMARY KEY (INDUSTRYID);

/*Printer table creation*/
CREATE TABLE PRINTER (
		PRINTERID INTEGER NOT NULL,
		PRINTERTYPE VARCHAR(255)
	);

CREATE UNIQUE INDEX SQL201007022330020 ON PRINTER (PRINTERID ASC);

ALTER TABLE PRINTER ADD CONSTRAINT SQL201007022330020 PRIMARY KEY (PRINTERID);

/*Staff_customer*/
CREATE TABLE STAFF_CUSTOMER (
		STAFF_STAFFID INTEGER NOT NULL,
		CUSTOMER_CUSTOMERID INTEGER NOT NULL
	);

CREATE INDEX SQL201007022331050 ON STAFF_CUSTOMER (STAFF_STAFFID ASC);

CREATE UNIQUE INDEX SQL201007022330400 ON STAFF_CUSTOMER (STAFF_STAFFID ASC, CUSTOMER_CUSTOMERID ASC);

CREATE INDEX SQL201007022330950 ON STAFF_CUSTOMER (CUSTOMER_CUSTOMERID ASC);

ALTER TABLE STAFF_CUSTOMER ADD CONSTRAINT SQL201007022330400 PRIMARY KEY (STAFF_STAFFID, CUSTOMER_CUSTOMERID);

ALTER TABLE STAFF_CUSTOMER ADD CONSTRAINT STFFCSTMRSTFFSTFFD FOREIGN KEY (STAFF_STAFFID)
	REFERENCES STAFF (STAFFID);

ALTER TABLE STAFF_CUSTOMER ADD CONSTRAINT STFFCSTCSTMRCSTMRD FOREIGN KEY (CUSTOMER_CUSTOMERID)
	REFERENCES CUSTOMER (CUSTOMERID);

/* Users table creation */
CREATE TABLE USERS (
		ID BIGINT NOT NULL,
		PASSWORD VARCHAR(255),
		USERNAME VARCHAR(255)
	);

CREATE UNIQUE INDEX SQL201007022330150 ON USERS (ID ASC);

ALTER TABLE USERS ADD CONSTRAINT SQL201007022330150 PRIMARY KEY (ID);

/* Group table creation */
CREATE TABLE GROUPS (
		ID BIGINT NOT NULL,
		GROUPNAME VARCHAR(255),
		USERNAME VARCHAR(255)
	);

CREATE UNIQUE INDEX SQL201007022330250 ON GROUPS (ID ASC);

ALTER TABLE GROUPS ADD CONSTRAINT SQL201007022330250 PRIMARY KEY (ID);



